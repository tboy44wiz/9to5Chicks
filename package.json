{
  "name": "9to5chick-server",
  "version": "1.0.0",
  "description": "9to5chick API",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "devstart": "nodemon --exec babel-node src/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "db:reset": "npm run migrate:undo && npm run migrate && npm run seed"
  },
  "author": "Tijani Femi",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@sendgrid/mail": "^6.5.5",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.11",
    "onesignal-node": "^3.1.5",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-replace-enum-postgres": "^1.5.0",
    "slugify": "^1.4.5",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "nyc": "^14.1.1"
  }
}
